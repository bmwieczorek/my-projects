<fileset-config file-format-version="1.2.0" simple-config="true" sync-formatter="false">
    <local-check-config name="Common Checkstyle" location=".checkstyle-checker.xml" type="project"
        description="">
        <additional-data name="protect-config-file" value="false" />
    </local-check-config>
    <fileset name="all" enabled="true" check-config-name="Common Checkstyle" local="true">
        <file-match-pattern match-pattern="^src[/\\]." include-pattern="true" />
    </fileset>
    <filter name="FileTypesFilter" enabled="true">
        <filter-data value="java" />
    </filter>
</fileset-config>
