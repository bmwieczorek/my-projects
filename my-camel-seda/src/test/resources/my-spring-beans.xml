<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:camel="http://camel.apache.org/schema/spring" xmlns:ticketing="http://www.sabre.com/schema/spring-ticketing-extensions"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
       http://camel.apache.org/schema/spring http://camel.apache.org/schema/spring/camel-spring.xsd
       http://www.sabre.com/schema/spring-ticketing-extensions http://www.sabre.com/schema/spring-ticketing-extensions-1.0.xsd">

    <bean id="mySpringRouteBuilder" class=" com.bawi.camel.intercept.MyCamelBeanMock.MySpringRouteBuilder"/>

    <bean id="springLoggingBean" class="com.bawi.camel.intercept.MyCamelBeanMock.SpringLoggingBean"/>
    
    <camel:camelContext xmlns="http://camel.apache.org/schema/spring" id="mySpringCamelContext">
        <camel:routeBuilder ref="mySpringRouteBuilder" />
    </camel:camelContext>


</beans>